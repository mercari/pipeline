{
  "sources": [
    {
      "name": "bigqueryInput",
      "module": "bigquery",
      "parameters": {
        "table": "myproject.mydataset.activities",
        "fields": ["userId", "createdAt", "event", "description"]
      },
      "timestampAttribute": "createdAt"
    }
  ],
  "sinks": [
    {
      "name": "bigtableOutput",
      "module": "bigtable",
      "inputs": ["bigqueryInput"],
      "parameters": {
        "projectId": "myproject",
        "instanceId": "myinstance",
        "tableId": "mytable",
        "rowKey": "${userId}#${_DateTimeUtil.formatTimestamp(createdAt, 'yyyyMMddHHmmSS', 'Asia/Tokyo')}",
        "columns": [
          {
            "family": "u",
            "qualifiers": [
              { "name": "id", "field": "userId" },
              { "name": "e", "field": "event" }
            ]
          },
          {
            "family": "e",
            "qualifiers": [
              { "name": "d", "field": "description" }
            ]
          }
        ],
        "timestampType": "event_timestamp"
      }
    }
  ]
}